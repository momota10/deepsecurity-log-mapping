                        }
                    }
                },
                "EventID": {
                    "type": "integer"
                },
                "EventType": {
                    "type": "text",
                    "fields": {
                        "keyword": {
                            "type": "keyword",
                            "ignore_above": 128
                        }
                    }
                },
                "LogDate": {
                    "type": "date"
                },
                "ManagerNodeID": {
                    "type": "long"
                },
                "ManagerNodeName": {
                    "type": "text",
                    "fields": {
                        "keyword": {
                            "type": "keyword",
                            "ignore_above": 256
                        }
                    }
                },
                "Number": {
                    "type": "integer"
                },
                "Origin": {
                    "type": "short"
                },
                "OriginString": {
                    "type": "text",
                    "fields": {
                        "keyword": {
                            "type": "keyword",
                            "ignore_above": 128
                        }
                    }
                },
                "Severity": {
                    "type": "short"
                },
                "SeverityString": {
                    "type": "text",
