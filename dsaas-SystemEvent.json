{
    "template": "dsaas-log-systemevent-*",
    "mappings": {
        "SystemEvent": {
            "properties": {
                "@timestamp": {
                    "type": "date"
                },
                "ActionBy": {
                    "type": "text",
                    "fields": {
                        "keyword": {
                            "type": "keyword",
                            "ignore_above": 256
                        }
                    }
                },
                "AdministratorID": {
                    "type": "long"
                },
                "Description": {
                    "type": "text",
                    "fields": {
